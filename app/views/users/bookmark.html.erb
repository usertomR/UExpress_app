<% provide(:title, "Article bookmark") %>

<div class="sum_question_article">
  <div>Article bookmark count / <%= @user.sum_articlebookmark_per_user.count %></div>
</div>
<div class="personal_question_article_list">
  <% if @user.sum_articlebookmark_per_user.any? %>
    <%= render partial: "shared/recorded_article_question_list_outline", locals: { one_type_instance_collection: @bookmarks, type: "articlebookmark" } %>
    <div class="pagenation">
      <%== pagy_bootstrap_nav(@pagy) %>
    </div>
  <% else %>
    <strong>記事のブックマークボタンを押していません</strong>
  <% end %>
</div>